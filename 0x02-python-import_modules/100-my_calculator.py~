#!/usr/bin/python3
if __name__ == "__main__":
    from calculator_1 import add, sub, mul, div
    import sys
    argv = sys.argv
    argv_len = len(argv)
    if (argv_len - 1) == 3:
        print("Usage: ./100-my_calculator.py <a> <operator> <b>")
        exit (1)
    operator = argv[2]
    if operator not in ['+', '-', '*', '/']:
        print("Unknown operator. Available operators: +, -, * and /")
        exit (1)
